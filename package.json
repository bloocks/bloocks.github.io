{
  "name": "bloocks",
  "version": "6.0.0",
  "description": "A static websites generator to build quickly marketing websites and ecommerce",
  "main": "gulpfile.babel.js",
  "devDependencies": {
    "autoprefixer": "9.8.6",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "browser-sync": "2.26.13",
    "cross-env": "7.0.2",
    "cross-spawn": "7.0.3",
    "cssnano": "4.1.10",
    "gulp": "4.0.2",
    "gulp-postcss": "^9.0.0",
    "postcss-import": "12.0.1",
    "tailwindcss": "1.9.6",
    "postcss": "^8.1.4",
    "postcss-cli": "^7.1.2",
    "rucksack-css": "^1.0.2",
    "gulp-cli": "^2.3.0",
    "postcss-load-config": "^3.0.0",
    "postcss-uncss": "^0.17.0",
    "uncss": "^0.17.3",
    "chalk": "^4.1.0",
    "minimatch": "^3.0.4",
    "replace": "^1.2.0",
    "yargs": "^16.1.1",
    "replace-in-file": "^6.1.0",
    "glob": "^7.1.6",
    "copyfiles": "^2.4.0",
    "imagemin-cli": "^6.0.0"
  },
  "scripts": {
    "build:production": "cross-env NODE_ENV=production gulp",
    "build:bloocksprod": "npm run build && replace-in-file \" </body> </html> \" \" \" \"_site/**/*.html\" && copyfiles -u 1 \"./_site/**/*\" ./docs && copyfiles -u 3 \"./_site/assets/css/style.css\" \"./_includes/bloocks/themes/amp/\"",
    "build:dev": "cross-env NODE_ENV=development gulp",
    "build": "npm run build:production",
    "dev": "cross-env NODE_ENV=development gulp serve",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bloocks/bloocks.github.io.git"
  },
  "author": "Antonio Trento",
  "license": "CC BY-NC-ND 2.0",
  "bugs": {
    "url": "https://github.com/bloocks/bloocks.github.io/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
